import socket

from appJson import run

print("стартуем сервер")
sock = socket.socket()
sock.bind(('', 9090))
sock.listen(1)
conn, addr = sock.accept()

print('connected:', addr)
listOfbooks = []
while True:
    data = conn.recv(1024)
    print(data)
    if not data:
        print("not data")
        break
    run(str(data.decode("utf-8")),listOfbooks)
    conn.send(bytearray(str(data.upper()) + "abc", "utf-8"))


conn.close()